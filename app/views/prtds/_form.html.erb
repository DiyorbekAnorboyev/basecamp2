<div class="container">
  <%= form_with(model: @prtd) do |form| %>
  <div class="text-center mt-3">
    <%= form.label :description, class:"fs-2" %><br>
    <%= form.text_field :description, class:"w-25 border rounded-3 border-5 " %>
  </div>
  <div>
    <%= form.text_field :project_id, type: :hidden %>
  </div>
  <div class="text-center mt-3 ">
    <%=  form.submit class:"text-light btn btn-info w-25" %>
  </div>
  <% end %>
</div>
