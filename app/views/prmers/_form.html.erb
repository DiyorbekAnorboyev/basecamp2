<%= form_with(model: prmer) do |form| %>
<div class=" mt-3">
  <%= form.label :name, class:"fs-2" %><br>
  <%= form.text_field :name, class:"w-25 border rounded-3 border-5 " %>
</div>
<div>
    <%= form.text_field :prm_id, type: :hidden %>
  </div>
<div class=" mt-3 ">
  <%=  form.submit "New Prmer", class: "text-light btn btn-info w-25" %>
</div>
<% end %>